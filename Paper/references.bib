@article{Feyrer2021,
abstract = {This paper exploits a temporary shock to distance, the closing of the Suez canal in 1967 and its reopening in 1975, to examine the effect of distance on trade and the effect of trade on income. Time series variation in sea distance allows for the inclusion of pair effects which account for static differences in tastes and culture between countries. Distance is found to have a significant impact on trade with an elasticity that is about half as large as estimates from typical cross sectional estimates. Since the shock to trade is exogenous for most countries, predicted trade volume from the shock can be used to identify the effect of trade on income. Trade is found to have a significant impact on income. The time series dimension allows for country fixed effects which control for all long run income differences. Because identification is through changes in sea distance, the effect is coming entirely through trade in goods and not through alternative channels such as technology transfer, tourism, or foreign direct investment.},
author = {Feyrer, James},
doi = {10.1016/j.jdeveco.2021.102708},
issn = {03043878},
journal = {Journal of Development Economics},
keywords = {Gravity,Growth,Suez canal,Trade},
